{
    "name": "webxid/laravel-linkedin",
    "type": "library",
    "description": "Linkedin API integration for Laravel and Lumen 5",
    "keywords": ["LinkedIn","Laravel Framework","Lumen","API","Client","SDK","Integration","REST"],
    "license": "MIT",
    "authors": [
        {
            "name": "Pavlo M.",
            "email": "webxid@ukr.net"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/contracts": "~5.0|^6.0|^7.0|^8.0|^9.0",
        "illuminate/http": "~5.0|^6.0|^7.0|^8.0|^9.0",
        "illuminate/support": "~5.0|^6.0|^7.0|^8.0|^9.0",
        "happyr/linkedin-api-client": "~1.0",
        "php-http/guzzle7-adapter": "^1.0",
        "php-http/curl-client": "^2.2",
        "guzzlehttp/psr7": "^1.4|^1.8.3|^2.1",
        "php-http/message": "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.5",
        "orchestra/testbench": "~3.4@dev"
    },
    "autoload": {
        "psr-4": {
            "Artesaos\\LinkedIn\\": "src/Artesaos/LinkedIn/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Artesaos\\LinkedIn\\Tests\\": "tests"
        }
    }
}
